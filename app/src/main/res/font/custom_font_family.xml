<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--regular-->
    <font
        android:font="@font/nanumsquareotfl"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanumsquareotfl"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute" />

    <!--bold-->
    <font
        android:font="@font/nanumsquareotfb"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/nanumsquareotfb"
        app:fontStyle="normal"
        app:fontWeight="1000"
        tools:ignore="UnusedAttribute" />

    <!--ebbold-->
    <font
        android:font="@font/nanumsquareotfeb"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/nanumsquareotfeb"
        app:fontStyle="normal"
        app:fontWeight="1000"
        tools:ignore="UnusedAttribute" />


</font-family>